{
  "namespace": "org.radarcns.phoneSensors",
  "type": "record",
  "name": "PhoneSensorUserInteraction",
  "doc": "Logs change of user interaction state: when the phone is unlocked or set to standby.",
  "fields": [
    {"name": "time", "type": "double", "doc": "device timestamp in UTC (s)"},
    {"name": "timeReceived", "type": "double", "doc": "device receiver timestamp in UTC (s)"},
    {"name": "interactionState", "type": "int", "doc": "0-standby, 1-unlocked"}
  ]
}